{
  "compilerOptions": {
    "target": "es5",
    "outDir": "./src",
    "declaration": false,
    "strict": true,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "incremental": true,
    "typeRoots": ["node_modules/@types"]
  },
  "types": ["node"],
  "excluce": ["node_modules"],
  "include": ["./tsrc/**/*"]
}

